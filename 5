a = float(input("please enter the first value a: "))
b = float(input("please enter the first value b: "))
c = float(input("please enter the first value c: "))

if(a > b):
    print("{0} is greater than {1}".format(a, b))
elif(b > c):
    print("{0} is greater than {1}".format(b, c))
elif(c > a):
    print("{0} is greater than {1}".format(c, a))
else:
    print("a, b, c are equal")
